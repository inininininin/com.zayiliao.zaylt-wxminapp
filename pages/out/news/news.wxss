@import '../../common/common.wxss';
page{
  background: rgb(245,245,245)
}
.news{
  width: 100%;
  height:218rpx;
  background: #fff;
  margin-top: 24rpx;
}
.news>navigator>view{
line-height: 108rpx;
  height: 108rpx;
}
.news>navigator>view:first-child{
  border-bottom: 1rpx solid #e5e5e5;
}
.news>navigator>view .Tbicon{
  float: left;
  width: 52rpx;
  height: 52rpx;
  margin: 28rpx 20rpx 28rpx 32rpx;
}
.news>navigator>view .rtIcon{
  float: right;
  width: 16rpx;
  height: 26rpx;
  margin: 41rpx 32rpx 41rpx 30rpx;
}
.news>navigator>view .num{
  width: 42rpx;
  height: 42rpx;
  float: right;
  margin: 33rpx 0;
  border-radius: 50%;
  background: rgb(255,233,233);
  color:  rgb(255,0,6);
  font-size: 24rpx;
  text-align: center;
  line-height: 42rpx;
}