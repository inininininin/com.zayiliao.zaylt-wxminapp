@import '../../common/common.wxss';
page{
  background: #f2f2f2;
}
.newList navigator>view{
  width: 702rpx;
  height: 190rpx;
  margin: 24rpx auto 0;
  background: #fff;
}
.newList navigator>view>view:first-child text:first-child{
  height: 34rpx;
  font-size: 34rpx;
  line-height: 34rpx;
  color: rgb(51,51,51);
  font-family: PingFangSC-Medium;
  display: inline-block;
  margin: 32rpx 30rpx 20rpx;
}
.newList navigator>view>view:first-child .point{
  width: 16rpx;
  height: 16rpx;
  display: inline-block;
  float: right;
  margin: 20rpx 20rpx 0 0;
  border-radius: 50%;
  background: rgb(255,26,46);
}
.newList navigator>view>view:nth-child(2){
  font-size: 30rpx;
  color: rgb(153,153,153);
  margin-left: 30rpx;
  margin-bottom: 20rpx;
  height: 30rpx;
  line-height: 30rpx;
}
.newList navigator>view>view:nth-child(3){
  font-size: 30rpx;
  color: rgb(51,51,51);
  margin-left: 30rpx;
   height: 30rpx;
  line-height: 30rpx;
  display:-webkit-box;
-webkit-line-clamp:1;
-webkit-box-orient:vertical;
overflow:hidden;
}