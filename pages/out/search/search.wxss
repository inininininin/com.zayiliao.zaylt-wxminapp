/* pages/out/search/search.wxss */
.container {
  box-sizing: border-box;
  /* height: 100%; */
  background: #fff;
}
.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
}
.status-bar {
  /* background: rgba(255, 0, 0, 0.1); */
}
.title-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.searchIcon {
  /* float: left; */
  position: absolute;
  width: 28rpx;
  height: 31rpx;
}
.back{
  width: 24rpx;
  height: 40rpx;
  position: absolute;
}
.title-bar view{
  height: 100%;
  width: 100%;
  /* text-align: center; */
  display: flex;
   /* justify-content: space-between; */
    align-items: center;
}
.title-bar view text{
  display: inline-block;
  width: 100%;
    margin: 0  0 0 -100rpx!important;
}
.titleText{
  font-size: 36rpx;
}
.search input{
  width: 350rpx;
  color: #000;
}
.search{
  width: 390rpx !important;
  /* padding-left: 30rpx; */
  margin-left: 82rpx;
  height: 60rpx !important;
  background: rgba(0,0,0,0.05);
  border-radius: 60rpx !important;
  font-size:30rpx;
  color: rgb(153,153,153);
  text-align: left !important;
  padding-left:80rpx;
}

page{
  width: 100%;
  /* height: 100%; */
  min-height: 100%;
  height: auto;  
  background: rgb(245,245,245);
  /* padding-bottom: 120rpx !Important; */
}
.indexbox{
  width: 702rpx;
  /* background: #fff; */
  border-radius: 6rpx;
  margin: 0 24rpx;
}
.indexbox navigator>view{
  height: 202rpx;
  background: #fff;
  margin-top: 24rpx;
}
.indexbox navigator>view>view{
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 30rpx;
  color: rgb(118,136,146);
  font-size: 30rpx;
}
.indexbox navigator>view>view input{
  color: #fff;
  float: right;
  width: 140rpx;
  height: 50rpx;
  background: rgb(43,119,239);
  text-align: center;
  border-radius: 6rpx;
  margin-top: 25rpx;
  font-size: 26rpx;
}
.indexbox navigator>view>view:first-child{
border-bottom: 2rpx solid #e5e5e5;
color: rgb(51,51,51);
}
.viewRt{
  float: right;
  line-height: 100rpx;
}
.indexbox navigator>view>view .viewRt image{
  width: 22rpx;
  height: 22rpx;
  margin-right: 10rpx; 
}
.indexbox navigator>view>view .viewRt text{
  font-size: 26rpx;
  color: rgb(43,119,239);
  line-height: 100rpx;
  display: inline-block;
}


.title-bar>view>.backHis{
  position: fixed;
  left: 0;
  width: 100rpx !important;
  height: 88rpx;
  float: left;
}