/* pages/promoter/clinicDetail/clinicDetail.wxss */
page {
  height: 100%;
}
.containers {
  box-sizing: border-box;
  /* height: 100%; */
  background: #fff;
  position: relative;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  /* color: #333; *//* background: #fff; */
  background: #fff;
  z-index: 888;
}

.status-bar {
  /* background: rgba(255, 0, 0, 0.1); */
}

.title-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.header .image {
  /* float: left; */
  position: absolute;
  width: 18rpx;
  height: 30rpx;
  z-index: 999;
  padding: 27rpx 25rpx;
}

.title-bar view {
  height: 100%;
  width: 100%;
  text-align: center;
  display: flex;
  /* justify-content: space-between; */
  align-items: center;
}

.title-bar view .text {
  display: inline-block;
  text-align: center;
  width: 100%;
  margin: 0 !important;
  position: absolute;
  left: 0;
}
.save>image {
  width: 38rpx;
  height: 40rpx;
  padding: 20rpx;
  margin-left: 90rpx;
  z-index: 999;
}
.addPatientBox{
  width: 100%;
  height: 114rpx;
  position: fixed;
  z-index: 2;
  background: #fff;
}
.addPatient{
  width:686rpx;
height:84rpx;
background:rgba(234,242,255,1);
border-radius:6rpx;
border:2rpx solid rgba(234,242,255,1);
margin: 10rpx 32rpx 20rpx;
  text-align: center;
  line-height: 84rpx;
  position: fixed;
}
.addPatient>image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.addPatient text{
  width:128rpx;
height:45rpx;
font-size:32rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(43,119,239,1);
line-height:45rpx;
}
.navbarBox{
  width: 100%;
  height: 147rpx;
  background: #fff;
  z-index: 2;
  position: fixed;
  margin-top: 115rpx;
}
.navbar{
  flex: none;
  display: flex;
  width:686rpx;
height:117rpx;
background:rgba(255,255,255,1);
box-shadow:0px 0px 26rpx 0px rgba(0,0,0,0.08);
border-radius:59rpx;
    margin: 15rpx 32rpx;
}
.navbar .item{
  width: 33.3%;
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 30rpx;
  color: #333333;
  font-size: 30rpx;
  font-family: PingFamhSc-Regular;
}
.navbar .item.active{
  color: #FF932E;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  /* top: 113rpx; */
  left: 98.5rpx;
  width: 32rpx;
  height: 4rpx;
  border-radius: 4rpx;
  background: #FF932E;
}
.navbar .item text{
  display: block;
  /* width:123rpx; */
height:42rpx;
font-size:30rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
/* color:rgba(255,147,46,1); */
line-height:42rpx;
}
.navbar .item text:nth-child(1){
  margin-top: 16rpx;
}

.indexbox{
  width: 702rpx;
  /* background: #fff; */
  border-radius: 6rpx;
  margin: 0 24rpx;

}
.indexbox navigator>view{
  height: 170rpx;
  background: #fff;
  margin-top: 24rpx;
  border-radius: 4rpx;
  box-shadow: 2rpx 2rpx 8rpx 0px rgba(0,0,0,0.1);
}
.indexbox navigator>view>image{
  width: 34rpx;
  height: 34rpx;
  float: left;
  margin: 20rpx 0 0 20rpx;
}
.indexbox navigator>view>view:nth-child(2){
  height: 170rpx;
  width: 440rpx;
  float: left;
  margin-left: 10rpx;
  font-size: 28rpx;
  color: rgb(51,51,51);
}
.indexbox navigator>view>view:nth-child(2) text{
  display: inline-block;
  margin: 10rpx 0 0 0;
}
.indexbox navigator>view>view:nth-child(2)>view{
  font-size: 30rpx;
  color: #000;
  margin-top:24rpx;
  height: 30rpx;
  line-height: 30rpx;
}
.indexbox navigator>view>view:nth-child(2)>view image{
  width: 26rpx;
  height: 16rpx;
}
.indexbox navigator>view .makesure{
  color: #fff;
  float: right;
  width: 160rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: rgb(43,119,239);
  text-align: center;
  border-radius: 28rpx;
  margin-top: 57rpx;
  margin-right: 20rpx;
  font-size: 24rpx;
}
.indexbox navigator>view .nosure{
color: rgb(51,51,51);
  float: right;
  width: 160rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: rgb(238,238,238);
  text-align: center;
  border-radius: 28rpx;
  margin-top: 57rpx;
  margin-right: 20rpx;
  font-size: 24rpx;
}
.indexbox navigator>view>view:first-child{
border-bottom: 2rpx solid #e5e5e5;
color: rgb(51,51,51);
}
.viewRt{
  float: right;
  line-height: 100rpx;
}
.indexbox navigator>view>view .viewRt image{
  width: 22rpx;
  height: 22rpx;
  margin-right: 10rpx; 
}
.indexbox navigator>view>view .viewRt text{
  font-size: 26rpx;
  color: rgb(43,119,239);
  line-height: 100rpx;
  display: inline-block;
}

.withoutNum{
  width: 700rpx !important;
  text-align: center;
}
.withoutNum image{
  width: 169rpx;
  height: 165rpx;
  margin: 150rpx 265rpx 20rpx;
}
.withoutNum text{
  width: 700rpx !important;
  display: block;
  font-size:30rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(51,51,51,1) !important;
}