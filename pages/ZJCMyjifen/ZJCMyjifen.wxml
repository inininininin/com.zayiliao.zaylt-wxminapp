<view class="container" style="padding-top:{{statusBarHeight+titleBarHeight}}px">
  <view class="header">
    <view class="status-bar" style="height:{{statusBarHeight}}px"></view>
    <view class="title-bar" style="height:{{titleBarHeight}}px">
    <view>
    <image  mode="aspectFill"  bindtap='backHistory' style='display:inline-block;top:{{2+statusBarHeight}}px;left:10rpx;padding:20rpx;'  src='../img/backArrow.png'></image>
    <text>{{navtitle}}</text>
    </view>
    </view>
  </view>
</view>
<!-- <view style='width:100%;height:24rpx;background:#f2f2f2;'></view> -->
<view class='lineTop'>
  <image  mode="aspectFill"  src='../img/lishijilu@2x.png'></image>
  <text>积分使用明细</text>
</view>
<view class='linelist' >
  <view class='line' wx:for="{{ schemeList }}"   wx:for-item="item" wx:key="index">
    <view class='lineone'>
      <text>{{item.note}}</text>
      <text>{{item.addTime}}</text>
    </view>
    <view class='linetwo'>{{item.amount}}</view>
  </view>
</view>


