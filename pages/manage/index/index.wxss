.navbar{
  width: 100%;
  flex: none;
  display: flex;
  /* background: #fff; */
    height: 44rpx;
    margin-top: 15rpx;
    position: fixed;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 30rpx;
  color: #fff;
  font-size: 30rpx;
  font-family: PingFamhSc-Regular;
}
.navbar .item.active{
  color: #fff;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  /* bottom: 0; */
  top: 56rpx;
  left: 102.5rpx;
  right: 102.5rpx;
  width: 48rpx;
  height: 6rpx;
  border-radius: 6rpx;
  background: #fff;
}
